<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context=".MainActivity">


    <Button
        android:id="@+id/id_btn_aidl_activity"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="aidl activity" />

    <Button
        android:id="@+id/id_btn_start_persist_to_file"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="persist to file" />

    <Button
        android:id="@+id/id_btn_start_Messenger_activity"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="start_messenger activity" />

    <Button
        android:id="@+id/id_btn_start_Socket_activity"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="start_socketr activity" />

    <Button
        android:id="@+id/id_btn_start_binder_pool_activity"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="start_binder_pool activity" />

    <Button
        android:id="@+id/id_btn_start_provider_activity"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="start_provider activity" />


</LinearLayout>